<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "https://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>

	<settings>
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>

	<typeAliases>
		<typeAlias type="com.jee.genesis.member.model.vo.Member" alias="member" />
		<typeAlias type="com.jee.genesis.admin.model.vo.Car" alias="car" />
		<typeAlias type="com.jee.genesis.admin.model.vo.CarName" alias="carName" />
		<typeAlias type="com.jee.genesis.admin.model.vo.Inventory" alias="inventory" />
	</typeAliases>

	<mappers>
		<mapper resource="/mappers/member-mapper.xml" />
		<mapper resource="/mappers/admin-mapper.xml" />
	</mappers>

</configuration>
